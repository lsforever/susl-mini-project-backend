{
    "name": "agro-backend",
    "version": "1.0.0",
    "description": "Node js Express server with mongodb as database for Agro applications.",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "node src/index.js",
        "background": "src/index.js &",
        "dev": "set NODE_ENV=development && nodemon --inspect src/index.js",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "@comment scripts": {
        "@script start": [
            "This is the start command.",
            "--inspect switch, listens for a debugging client. By default, it will listen at host and port 127.0.0.1:9229"
        ],
        "@script background": [
            "This will run server as a background process.",
            "&, will do the trick."
        ],
        "@script dev": "This will run server with -development- env."
    },
    "keywords": [],
    "author": "lsforever",
    "license": "ISC",
    "dependencies": {
        "@google-cloud/storage": "^7.1.0",
        "@turf/boolean-point-in-polygon": "^6.5.0",
        "@turf/helpers": "^6.5.0",
        "accesscontrol": "^2.2.1",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-healthcheck": "^0.1.0",
        "express-rate-limit": "^6.9.0",
        "express-validator": "^7.0.1",
        "google-auth-library": "^9.0.0",
        "http-status-codes": "^2.2.0",
        "jsonwebtoken": "^9.0.1",
        "mime-types": "^2.1.35",
        "mongoose": "^7.4.1",
        "mongoose-paginate-v2": "^1.7.31",
        "mongoose-to-swagger": "^1.5.1",
        "morgan": "^1.10.0",
        "multer": "1.4.5-lts.1",
        "passport": "^0.6.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
